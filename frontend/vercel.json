{
  "rewrites": [
    {
      "source": "/download",
      "destination": "/index.html"
    },
    {
      "source": "/4k-videos",
      "destination": "/index.html"
    },
    {
      "source": "/free-youtube-downloader",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "trailingSlash": false
}